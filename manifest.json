{
  "name": "Music Youtube",
  "description": "nothing",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage", "tabs", "bookmarks", "declarativeWebRequest", "*://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Music Youtube",
    "default_popup": "popup.html"
  },
  "commands": {
    "next": {
      "description": "next",
      "suggested_key": { "default": "Ctrl+Space" }
    },
    "global_next2": {
      "description": "next",
      "suggested_key": { "default": "Ctrl+Shift+0" },
      "global":true
    },
    "pause": {
      "description": "pause",
      "suggested_key": { "default": "Ctrl+Shift+Space" }      
    },
    "pause_global": {
      "description": "pause",
      "suggested_key": { "default": "Ctrl+Shift+2" },
      "global":true
    }
  }
}
